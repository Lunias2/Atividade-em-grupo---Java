package jogo;

public class Peao {
	private String cor;
	private int linha;
	private int coluna;
	
	/**
	 * @param cor
	 * @param linha
	 * @param coluna
	 */
	public Peao(String cor, int linha, int coluna) {
		this.cor = cor;
		this.linha = linha;
		this.coluna = coluna;
	}

	/**
	 * 
	 */
	public Peao() {
	}
	
	
	
	
	
	
	
}
